{
  "name": "tokenparser-api-shared",
  "version": "1.0.0",
  "description": "Shared Token Parser Express API implementation for DEMOAPP stacks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "default": "./dist/api/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "start": "tsx src/start.ts",
    "dev": "tsx watch src/start.ts",
    "test": "npm run build"
  },
  "keywords": ["tokenparser", "express", "api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "express": "^4.21.0",
    "js-yaml": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.7",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  }
}
