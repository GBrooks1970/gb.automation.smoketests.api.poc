  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "ts:check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"screenplay/**/*.ts\" \"cypress/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*.ts\" \"screenplay/**/*.ts\" \"cypress/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"screenplay/**/*.ts\" \"cypress/**/*.ts\"",
    "test:bdd": "cypress run",
    "test": "npm run test:bdd",
    "verify": "npm run ts:check && npm run test:bdd",
    "cy:test": "cypress open",
    "postinstall": "npx cypress verify"
  },
