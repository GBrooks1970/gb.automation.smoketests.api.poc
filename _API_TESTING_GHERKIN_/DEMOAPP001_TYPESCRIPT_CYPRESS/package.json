    "@types/chai": "^5.2.3",
    "@types/node": "^22.5.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.4.0",
    "prettier": "^3.3.3",
    "cypress": "^13.17.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
