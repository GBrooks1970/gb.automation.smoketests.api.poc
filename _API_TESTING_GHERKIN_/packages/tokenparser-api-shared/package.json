{
  "name": "@demoapps/tokenparser-api-shared",
  "version": "1.0.0",
  "description": "Shared Token Parser API implementation reused by DEMOAPP001 and DEMOAPP003",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "parsers": [
        "dist/parsers.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./parsers": {
      "types": "./dist/parsers.d.ts",
      "default": "./dist/parsers.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "js-yaml": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.6.2",
    "vitest": "^2.1.3"
  }
}
